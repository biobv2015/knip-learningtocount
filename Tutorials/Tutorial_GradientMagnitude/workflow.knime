<entry key="x-coordinate" type="xint" value="130"/>
<entry key="y-coordinate" type="xint" value="356"/>
<entry key="width" type="xint" value="121"/>
<entry key="height" type="xint" value="122"/>
<entry key="alignment" type="xstring" value="LEFT"/>
<config key="styles"/>
</config>
<config key="annotation_2">
<entry key="text" type="xstring" value="With the Global Thresholder (OTSU-Method) we extract our cellclumps. We now have a binary image."/>
